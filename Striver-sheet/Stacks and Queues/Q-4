//GFG: Implement a Queue using two stack s1 and s2.
//https://practice.geeksforgeeks.org/problems/queue-using-stack/1

class Queue {
stack<int> input, output;
public:

    void enqueue(int x) {
        input.push(x);
    }

    int dequeue() {
        int x;
        if(output.empty()){
            if(input.empty()) return -1;
            while(!input.empty()){
                output.push(input.top());
                input.pop();
            }
        }
        x = output.top();
        output.pop();
        return x;
    }
};
