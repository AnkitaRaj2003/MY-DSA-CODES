//GFG: Floor in a bst
//https://practice.geeksforgeeks.org/problems/floor-in-bst/1

    int floor(Node* root, int x) {
        // Code here
        int ans = -1;
        while(root){
            if(root->data == x) return root->data;
            else if(x > root->data){
                ans = root->data;
                root = root->right;
            }
            else root = root->left;
        }
        return ans;
    }
