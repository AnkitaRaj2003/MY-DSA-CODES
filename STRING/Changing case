//Uppercase to Lowercase and vice-versa

#include<iostream>
#include<string>
using namespace std;

int main(){
    char c[] = "Ankita Meena";
    for(int i=0;c[i]!='\0';i++){
        if(c[i]>=65 && c[i]<=90) c[i]+=32;
        else if(c[i]>=97 && c[i]<=122) c[i]-=32;
    }
    cout<<c;
    return 0;
}
