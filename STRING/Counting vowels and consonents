//Counting vowels and consonents

#include<iostream>
#include<string>
using namespace std;

int main(){
    char c[] = "ankita meena";
    int x=0,v=0;
    for(int i=0;c[i]!='\0';i++){
        if(c[i]=='a' || c[i]=='e' || c[i]=='i' || c[i]=='o' || c[i]=='u' || c[i]=='A' || c[i]=='E' || c[i]=='I' || c[i]=='O' || c[i]=='U') v++;
        else if((c[i]>=65 && c[i]<=90) || (c[i]>=97 && c[i]<=122)) x++; 
    }
    cout<<"Vowels count: "<<v<<"\nConsonents count: "<<x;
    return 0;
}
