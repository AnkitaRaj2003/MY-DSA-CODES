//Leetcode: 

----------------------------------------------------------------------------------------
TC: O()    SC: O(N)
    int f(int i,vector<int>v,int c){
        if(i==v.size()-1) return c;
        int mini = INT_MAX;
        for(int j=1;j<=v[i] && i+j<v.size();j++) mini = min(mini,f(i+j,v,c+1));
        return mini;
    }
    int jump(vector<int>& nums) {
       return f(0,nums,0); 
    }


-------------------------------------BETTER---------------------------------------------
